{
  "name": "barelyjs",
  "version": "1.1.2",
  "description": "A minimalistic frontend library for building UI components",
  "main": "dist/barelyjs.cjs.js",
  "module": "dist/barelyjs.esm.js",
  "browser": "dist/barelyjs.iife.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/barelyjs.esm.js",
      "require": "./dist/barelyjs.cjs.js",
      "browser": "./dist/barelyjs.iife.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "barelyjs",
    "frontend",
    "ui",
    "library",
    "javascript",
    "framework",
    "minimal"
  ],
  "author": {
    "name": "NikitaBerezhnyj",
    "email": "nikita.berezhnyj@gmail.com",
    "url": "https://github.com/NikitaBerezhnyj"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikitaBerezhnyj/BarelyJS.git"
  },
  "bugs": {
    "url": "https://github.com/NikitaBerezhnyj/BarelyJS/issues"
  },
  "homepage": "https://github.com/NikitaBerezhnyj/BarelyJS#readme",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup": "^4.57.1",
    "typescript": "^5.9.3"
  },
  "type": "module",
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
